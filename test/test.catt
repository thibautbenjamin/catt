let sq (x : *) (f : x -> x) = comp x x f x f

let cbd (x : *) (f : x -> x) : x -> x = comp x x f x (comp x x f x f)

set verbosity = 3

coh simpl (x : *) : sq x (id x) -> id x

check (x : *) (f : x -> x) = comp x x (sq x f) x (cbd x f)

coh failure (x(f)y(g)z) : x -> y

